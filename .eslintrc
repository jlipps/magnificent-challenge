{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "settings": {
        "ecmascript": 6,
        "jsx": true
    },
    "ecmaFeatures": {
        "jsx": true
    },
    "plugins": [
        "react"
    ],
    "extends": "eslint:recommended",
    "rules": {
        "space-before-function-paren": 2,
        "no-console": 1
    },
    "globals": {
        "describe": false,
        "before": false,
        "after": false,
        "it": false
    }
}
